CREATE TABLE spotify_data.db (INSERT INTO spotify_data (ts, username, platform, ms_played, conn_country, ip_addr_decrypted, user_agent_decrypted, master_metadata_track_name, master_metadata_album_artist_name, master_metadata_album_album_name, spotify_track_uri, episode_name, episode_show_name, spotify_episode_uri, reason_start, reason_end, shuffle, skipped, offline, offline_timestamp, incognito_mode);
CREATE TABLE spotify_data (
    ts TEXT,
    username TEXT,
    platform TEXT,
    ms_played INTEGER,
    conn_country TEXT,
    ip_addr_decrypted TEXT,
    user_agent_decrypted TEXT,
    master_metadata_track_name TEXT,
    master_metadata_album_artist_name TEXT,
    master_metadata_album_album_name TEXT,
    spotify_track_uri TEXT,
    episode_name TEXT,
    episode_show_name TEXT,
    spotify_episode_uri TEXT,
    reason_start TEXT,
    reason_end TEXT,
    shuffle BOOLEAN,
    skipped TEXT,
    offline BOOLEAN,
    offline_timestamp INTEGER,
    incognito_mode BOOLEAN);
.exit
CREATE TABLE dateabase (
   id INTEGER PRIMARY KEY, 
   name TEXT,
   age INTEGER,
   app TEXT,
   height INTEGER,
   alcohol TEXT,
   smoking TEXT,
   job TEXT,
   country_of_origin TEXT,
   num_pictures INTEGER,
   num_prompts INTEGER,
   num_emojis INTEGER,
   insta_phone BOOLEAN,
   no_dates INTEGER,
);
